<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.br.controlecondominio"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="27" />

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.br.controlecondominio.activitys.ListaMoradorActivity" />
        <activity android:name="com.br.controlecondominio.activitys.FormMoradorActivity" />
        <activity
            android:name="com.br.controlecondominio.activitys.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.br.controlecondominio.activitys.ListaPrestadorActivity" />
        <activity android:name="com.br.controlecondominio.activitys.FormPrestadorActivity" />
        <activity android:name="com.br.controlecondominio.activitys.FormReceitaActivity" />
        <activity android:name="com.br.controlecondominio.activitys.ListaReceitaActivity" />
        <activity android:name="com.br.controlecondominio.activitys.FormTotalizadorActivity" />
        <activity android:name="com.br.controlecondominio.activitys.ListaDespesaActivity" />
        <activity android:name="com.br.controlecondominio.activitys.FormDespesaActivity" />

        <provider
            android:name="com.android.tools.ir.server.InstantRunContentProvider"
            android:authorities="com.br.controlecondominio.com.android.tools.ir.server.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>